{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "css.lint.unknownAtRules": "ignore",
  "less.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "windicss.enableEmmetCompletion": true,
  "windicss.sortOnSave": true,
  "windicss.enableRemToPxPreview": true,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.inlayHints.enabled": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "eslint.autoFixOnSave": true
  },
  // finished
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.startupEditor": "none",
  "html.format.wrapLineLength": 110,
  "vue3snippets.printWidth": 110,
  "vue3snippets.singleQuote": true,
  "editor.accessibilitySupport": "on",
  "editor.defaultFormatter": "passionkind.prettier-vscode-with-tabs",
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false
}

